{
    "name": "rmzamora/media-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataMediaBundle rmzamora fork",
    "keywords": ["media", "sonata", "youtube", "dailymotion", "vimeo", "pdf", "file", "upload"],
    "homepage": "https://sonata-project.org/bundles/media",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Mell M. Zamora",
            "email": "me@mellzamora.com",
            "homepage": "http://mellzamora.com"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataMediaBundle/contributors"
        }
    ],
    "require": {
        "symfony/symfony": "~2.4",
        "sonata-project/core-bundle": "^2.3.1",
        "sonata-project/easy-extends-bundle": "~2.1",
        "sonata-project/doctrine-extensions": "~1.0",
        "knplabs/gaufrette": ">=0.1.6",
        "imagine/Imagine": "*@stable",
        "kriswallsmith/buzz": "0.*",
        "friendsofsymfony/rest-bundle": "~1.1",
        "jms/serializer-bundle": "~0.11|~1.0",
        "nelmio/api-doc-bundle": "~2.4",
        "sonata-project/intl-bundle": "~2.2",
        "rmzamora/notification-bundle": "2.4.1.*",
        "rmzamora/classification-bundle": "2.4.1.*"
    },
    "require-dev": {
        "rmzamora/doctrine-orm-admin-bundle": "2.4.1.*",
        "rmzamora/formatter-bundle": "2.4.1.*",
        "rmzamora/seo-bundle": "2.4.1.*",
        "sonata-project/core-bundle": "~2.2@dev",
        "sonata-project/datagrid-bundle": "~2.2@dev",
        "aws/aws-sdk-php": "~2.7",
        "doctrine/mongodb-odm": "~1.0@dev",
        "jackalope/jackalope-doctrine-dbal": "~1.1",
        "symfony/phpunit-bridge": "~2.7|~3.0",
        "fabpot/php-cs-fixer": "~0.1|~1.0"
    },
    "suggest": {
        "rmzamora/doctrine-orm-admin-bundle": "2.4.1.*",
        "liip/imagine-bundle": "0.9.*",
        "rackspace/php-opencloud": "~1.6",
        "tilleuls/ckeditor-sonata-mediabundle": "~1.0"
    },
    "conflict": {
        "jms/serializer": "<0.13",
        "sonata-project/seo-bundle": "<1.1.5",
        "sonata-project/block-bundle": "<2.2.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "Sonata\\MediaBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-2.4.1": "2.4.1.x-dev"
        }
    }
}
