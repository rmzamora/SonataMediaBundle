{
    "name": "rmzamora/media-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataMediaBundle rmzamora fork",
    "keywords": ["media", "sonata", "youtube", "dailymotion", "vimeo", "pdf", "file", "upload"],
    "homepage": "http://sonata-project.org/bundles/media",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "http://sonata-project.org"
        },
        {
            "name": "Mell M. Zamora",
            "email": "me@mellzamora.com",
            "homepage": "http://mellzamora.com"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataMediaBundle/contributors"
        }
    ],
    "require": {
        "symfony/symfony": "~2.3",
        "rmzamora/notification-bundle": "~2.3",
        "sonata-project/easy-extends-bundle": "2.1.*",
        "sonata-project/doctrine-extensions": "1.*",
        "knplabs/gaufrette": ">=0.1.4",
        "imagine/Imagine": "*@stable",
        "kriswallsmith/buzz": "0.*"
    },
    "require-dev": {
        "rmzamora/doctrine-orm-admin-bundle": "~2.3",
        "rmzamora/formatter-bundle": "~2.3",
        "amazonwebservices/aws-sdk-for-php": "1.5.*"
    },
    "suggest": {
        "liip/imagine-bundle": "0.9.*",
        "amazonwebservices/aws-sdk-for-php": "1.5.*",
        "rackspace/php-opencloud": "1.6.*"
    },
    "autoload": {
        "psr-0": { "Sonata\\MediaBundle": "" }
    },
    "target-dir": "Sonata/MediaBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.3.x-dev"
        }
    }
}
